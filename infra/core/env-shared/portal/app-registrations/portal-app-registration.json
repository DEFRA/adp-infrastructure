{
    "applications": [
        {
            "displayName": "#{{ portalAppRegName }}",
            "keyVault": {
                "name": "#{{ ssvPortalKeyVaultName }}",
                "secrets": [
                    {
                        "key": "#{{ portalApplicationClientSecretName }}",
                        "type": "ClientSecret"
                    },
                    {
                        "key": "#{{ portalApplicationClientIdName }}",
                        "type": "ClientId"
                    }                    
                ]
            },
            "web": {
                "redirectUris": [
                    "http://localhost:7007/api/auth/microsoft/handler/frame",
                    "https://dev-portal.snd1.adp.defra.gov.uk/api/auth/microsoft/handler/frame",
                    "https://portal.snd1.adp.defra.gov.uk/api/auth/microsoft/handler/frame"
                ],
                "implicitGrantSettings": {
                    "enableAccessTokenIssuance": true,
                    "enableIdTokenIssuance": false
                }
            }
            
        }
    ]
}