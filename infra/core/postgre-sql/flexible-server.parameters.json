{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "server": {
            "value": {
                "name": "#{{ dbsResourceNamePrefix }}#{{ nc_resource_postgresql }}#{{ nc_instance_regionid }}01A",
                "storageSizeGB": #{{ postgreSqlStorageSizeGB }},
                "tier": "#{{ postgreSqlTier }}",
                "skuName": "#{{ postgreSqlSkuName }}",
                "highAvailability": "#{{ postgreSqlHighAvailability }}",
                "availabilityZone": "#{{ postgreSqlAvailabilityZone }}"
            }
        },
        "vnet": {
            "value": {
              "name": "#{{ virtualNetworkName }}",
              "resourceGroup": "#{{ virtualNetworkResourceGroup }}",
              "subnetPostgreSql": "#{{ networkResourceNamePrefix }}#{{ nc_resource_subnet }}#{{ nc_instance_regionid }}05"
            }
        },
        "privateDnsZoneName":{
            "value": "${{ dnsResourceNamePrefix }}${{ nc_resource_dnszone }}${{ nc_instance_regionid }}02.private.postgres.database.azure.com"
        },
        "diagnostics": {
            "value": {
                "diagnosticLogCategoriesToEnable": [
                    "allLogs"
                ],
                "diagnosticMetricsToEnable": [
                    "AllMetrics"
                ]
            }
        },
        "location": {
            "value": "#{{ location }}"
        },
        "environment": {
            "value": "#{{ environment }}"
        }
    }
}