{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "#{{ infraResourceNamePrefix }}#{{ nc_resource_frontdoor }}#{{ nc_instance_regionid }}01"
    },
    "sku": {
      "value": "#{{ cdnProfileSkuName }}"
    },
    "location": {
      "value": "global"
    },
    "environment": {
      "value": "#{{ environment }}"
    },
    "endpoints":{
      "value": [
       {
        "name": "#{{ environment }}#{{ environmentId }}-adp-cluster"
       } 
      ]
    },
    "ruleSets": {
      "value": [
        {
          "name": "ResponseHeaderRuleSet",
          "rules": [
            {
              "name": "AppendStrictTransportSecurityHeader",
              "order": 0,
              "actions": [
                {
                  "name": "ModifyResponseHeader",
                  "parameters": {
                      "typeName": "DeliveryRuleHeaderActionParameters",
                      "headerAction": "Append",
                      "headerName": "Strict-Transport-Security",
                      "value": "max-age=31536000; includeSubDomains"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}