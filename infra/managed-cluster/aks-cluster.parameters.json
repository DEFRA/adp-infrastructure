{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vnet": {
            "value": {
                "name": "#{{ virtualNetworkName }}",
                "resourceGroup": "#{{ virtualNetworkResourceGroup }}",
                "subnetClusterNodes": "#{{ networkResourceNamePrefix }}#{{ nc_resource_subnet }}#{{ nc_instance_regionid }}07"
            }
        },
        "cluster": {
            "value": {
                "name": "#{{ infraResourceNamePrefix }}#{{ nc_resource_kubernetesservice }}#{{ nc_instance_regionid }}01",
                "skuTier": "#{{ aksClusterSkuTier }}",
                "nodeResourceGroup": "#{{ aksNodeResourceGroup }}",
                "miControlPlane": "#{{ aksControlPlaneManagedIdentity }}",
                "adminAadGroupObjectId": "#{{ aksAADProfileAdminGroupObjectId }}",
                "npSystem":{
                    "count": 1,
                    "vmSize": "Standard_DS2_v2",
                    "type": "VirtualMachineScaleSets",
                    "osDiskSizeGB": 80,
                    "osDiskType": "Ephemeral",
                    "osType": "Linux",
                    "osSKU": "Ubuntu",
                    "availabilityZones":["1"]
                },
                "npUser":{
                    "count": 2,
                    "vmSize": "Standard_DS3_v2",
                    "type": "VirtualMachineScaleSets",
                    "osDiskSizeGB": 128,
                    "osDiskType": "Ephemeral",
                    "osType": "Linux",
                    "osSKU": "Ubuntu",
                    "maxCount": 3,
                    "maxPods": 30,
                    "minCount": 1,
                    "minPods": 2,
                    "scaleSetEvictionPolicy": "Delete",
                    "scaleSetPriority": "Regular",
                    "storageProfile": "ManagedDisks",
                    "availabilityZones":["1","2","3"]
                }
            }
        },
        "location": {
            "value": "#{{ location }}"
        },
        "environment": {
            "value": "#{{ environment }}"
        }
    }
}