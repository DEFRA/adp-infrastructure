{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vnet": {
      "value": {
        "name": "#{{ virtualNetworkName }}",
        "resourceGroup": "#{{ virtualNetworkResourceGroup }}"
      }
    },
    "flowLogs": {
      "value": {
        "name": "NetworkWatcher_uksouth/#{{ infraResourceNamePrefix }}#{{ nc_resource_flowlog }}#{{ nc_instance_regionid }}01"
      }
    },
    "storageAccount": {
      "value": {
        "name": "#{{ infraResourceNamePrefix }}#{{ nc_resource_storageaccount }}#{{ nc_instance_regionid }}01"
      }
    },
    "location": {
      "value": "#{{ location }}"
    },
    "environment": {
      "value": "#{{ environment }}"
    },
    "servicesResourceGroup": {
      "value": "#{{ servicesResourceGroup }}"
    }
  }
}

