{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "buildAgent": {
      "value": {
        "name": "#{{ infraResourceNamePrefix }}#{{ nc_resource_virtualmachine_scalesets }}#{{ nc_instance_regionid }}01-#{{ imageType }}",
        "userName": "azureuser",
        "sku": "Standard_D4s_v4",
        "imageReference": {
          "id": "/subscriptions/#{{ imageGallerySubscription }}/resourceGroups/#{{ imageGalleryResourceGroup }}/providers/Microsoft.Compute/galleries/#{{ imageGalleryName }}/images/#{{ imageType }}"
        },
        "osDisk": {
          "createOption": "FromImage",
          "caching": "ReadWrite",
          "managedDisk": {
            "storageAccountType": "Premium_LRS"
          },
          "diskSizeGB": 128
        },
        "osType": "Linux"
      }
    },
    "nicConfigurations": {
      "value": [
        {
          "nicSuffix": "-nic",
          "ipConfigurations": [
            {
              "name": "ipConfig",
              "properties": {
                "subnet": {
                  "id": "/subscriptions/#{{ subscriptionId }}/resourceGroups/#{{ virtualNetworkResourceGroup }}/providers/Microsoft.Network/virtualNetworks/#{{ virtualNetworkName }}/subnets/#{{ networkResourceNamePrefix }}#{{ nc_resource_subnet }}#{{ nc_instance_regionid }}03"
                }
              }
            }
          ]
        }
      ]
    },
    "adminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/7dc5bbdf-72d7-42ca-ac23-eb5eea3764b4/resourceGroups/SSVCDOINFRG3401/providers/Microsoft.KeyVault/vaults/SSVCDOINFVT3401"
        },
        "secretName": "ADO-BuildAgent-Password"
      }
    },
    "environment": {
      "value": "#{{ environment }}"
    }
  }
}