{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "value": "#{{ ssvResourceNamePrefix }}#{{ nc_resource_grafana }}#{{ nc_shared_instance_regionid }}01"
      },
      "publicNetworkAccess": {
        "value": "Enabled"
      },
      "zoneRedundancy": {
        "value": "Disabled"
      },
      "managedServiceIdentityType": {
        "value": "SystemAssigned"
      },
      "azureMonitorWorkspaceResourceIds": {
        "value": "#{{ azureMonitorWorkspaceResourceIds }}"
      },
      "location": {
        "value": "#{{ location }}"
      },  
      "environment": {
        "value": "#{{ environment }}"
      },
      "azureMonitorWorkspaceResourceIds": {
        "value": #{{ noescape(grafanaLinkToAzureMonitorWorkspace) }}
      }
    }
  }